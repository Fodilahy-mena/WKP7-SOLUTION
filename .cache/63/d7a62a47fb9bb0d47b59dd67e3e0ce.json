d;\n\tgap: 10px;\n}\n\n/* FORM  */\n\nform {\n\tdisplay: grid;\n\talign-items: end;\n\tgrid-template-columns: repeat(5, 3fr) auto;\n\tgap: 20px;\n}\n\ninput {\n\tborder: none;\n\tbox-shadow: inset 0px 1px 2px rgba(0, 0, 0, 0.1);\n\tbackground-color: var(--grey-light);\n\tpadding: 0.6rem 1rem;\n\twidth: 100%;\n\tborder-radius: 5px;\n\tmargin-top: 0.3rem;\n\tfont-size: 14px;\n}\n\nselect {\n\tborder: none;\n\tbox-shadow: inset 0px 1px 2px rgba(0, 0, 0, 0.1);\n\tbackground-color: var(--grey-light);\n\theight: 2.25rem;\n\twidth: 100%;\n\tborder-radius: 5px;\n\tcolor: var(--grey-writing);\n\tmargin-top: 0.3rem;\n\tfont-size: 14px;\n}\n\nlabel {\n\tfont-size: 11px;\n\tline-height: 15px;\n\tcolor: var(--black);\n}\n\nfieldset button {\n\tborder: none;\n\tborder-radius: 5px;\n\tbackground-color: var(--blue);\n\tcolor: white;\n\tfont-size: 14px;\n\ttext-transform: uppercase;\n\tpadding: 0.6rem 2rem;\n\tcursor: pointer;\n\tfilter: drop-shadow(2px 2px 5px rgba(0, 0, 0, 0.1));\n}\n\nfieldset button:hover {\n\tfilter: drop-shadow(0px 4px 6px rgba(0, 0, 0, 0.2));\n}\n\n/* TABLE */\n\ntable {\n\tbackground: white;\n\tdisplay: grid;\n\toverflow: scroll;\n}\n\nthead {\n\tbackground: rgba(0, 127, 189, 0.03);\n\tborder-radius: 10px 10px 0px 0px;\n\ttext-transform: uppercase;\n\tletter-spacing: 0.11em;\n\tfont-weight: 600;\n\tcolor: var(--grey-writing);\n\tfont-size: 14px;\n}\n\ntbody {\n\tdisplay: grid;\n}\n\nth,\ntd {\n\tpadding: 2rem 0.5rem;\n\talign-self: center;\n\tfont-size: 14px;\n\tline-height: 17px;\n}\n\ntr {\n\tdisplay: grid;\n\tjustify-items: center;\n\tgrid-template-columns: 20% 20% 15% 15% 15% 15%;\n\tborder-bottom: 1px solid var(--grey-bg);\n\tpadding: 0 2rem;\n}\n\ntr button {\n\tborder: none;\n\tbackground: none;\n\tcursor: pointer;\n\tpadding: 0rem;\n}\n\ntr button:hover {\n\tcolor: var(--grey-writing);\n}\n\n.left {\n\tjustify-self: left;\n}\n"},"lineCount":null}},"error":null,"hash":"b7661f43c6fccbdb4acc5cef5a9e7490","cacheData":{"env":{}}}